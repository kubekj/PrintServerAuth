{
  "roles": {
    "User": {
      "permissions": ["print", "queue"]
    },
    "PowerUser": {
      "inherits": "User",
      "permissions": ["topQueue", "restart"]
    },
    "Technician": {
      "permissions": ["start", "stop", "restart", "status", "readConfig", "setConfig"]
    },
    "Admin": {
      "inherits": ["Technician", "PowerUser"],
      "permissions": ["all"]
    }
  }
}
